{
    "name": "fadlee/svelte-wp-starter",
    "description": "A WordPress Svelte starter plugin",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Fadlul Alim",
            "email": "fad.lee@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "includes/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "5.4.x-dev"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "install-prod": "composer install --no-dev"
    }
}
